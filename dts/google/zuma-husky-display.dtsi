#include <dt-bindings/gpio/gpio.h>

&drmdsim0 {
	google_hk3: panel@0 {
		compatible = "google,hk3";
		label = "google-hk3";
		channel = <0>;
		//touch = <&spitouch>;

		/* reset, power */
		reset-gpios = <&gpa6 0 GPIO_ACTIVE_HIGH>;
		vci-supply = <&m_ldo14_reg>;
		vddi-supply = <&s_bucka_reg>;
		vddd-supply = <&m_ldo22_reg>;
	};
	panel@1 {
		compatible = "samsung,emul";
		label = "samsung-emul";
		channel = <0>;

		/* reset, power */
		reset-gpios = <&gpa6 0 GPIO_ACTIVE_HIGH>;
	};
};
